import java.util.*;
class CountDigits {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter a number:");
        int n=sc.nextInt();
        int count=0;
        while(n>0){
            int rem=n%10;
            n/=10;
            count++; 
        }
       System.out.println("count: "+count); 
    }
}

